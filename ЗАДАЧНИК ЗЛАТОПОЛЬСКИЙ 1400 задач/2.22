a = float(input("Enter first number: "))
b = float(input("Enter second number: "))
mod_a = abs(a)
mod_b = abs(b)

# вычисление среднего арифметического
mean_arith = (mod_a + mod_b) / 2

# вычисление среднего геометрического
mean_geom = (mod_a * mod_b)**(1/2)

# вывод результатов
print("Mean arithmetic: ", mean_arith)
print("Mean geometric: ", mean_geom)